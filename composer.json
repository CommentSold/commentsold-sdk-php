{
    "name": "commentsold/commentsold-sdk-php",
    "description": "CommentSold SDK",
    "keywords": [
        "CommentSold",
        "SDK"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^9.5.10",
        "phpstan/phpstan": "< 1.9",
        "phpstan/phpstan-mockery": "^1.1",
        "laravel/pint": "1.10.5",
        "qossmic/deptrac-shim": "^1.0",
        "brianium/paratest": "^6"
    },
    "autoload": {
        "psr-4": {
            "CommentSold\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
